# Stripe Configuration
# Récupérez ces clés depuis votre dashboard Stripe
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key_here
STRIPE_SECRET_KEY=sk_test_your_secret_key_here

# EmailJS Configuration
# Créez un compte EmailJS et configurez un service email
NEXT_PUBLIC_EMAILJS_SERVICE_ID=your_service_id_here
NEXT_PUBLIC_EMAILJS_TEMPLATE_ID=your_template_id_here
NEXT_PUBLIC_EMAILJS_USER_ID=your_user_id_here

# Formspree Configuration
# Créez des formulaires sur Formspree pour le contact et l'inscription
NEXT_PUBLIC_FORMSPREE_CONTACT_FORM_ID=your_contact_form_id_here
NEXT_PUBLIC_FORMSPREE_REGISTRATION_FORM_ID=your_registration_form_id_here

# Site Configuration
NEXT_PUBLIC_SITE_URL=https://pkba.vertiflow.fr
NEXT_PUBLIC_SITE_NAME=PKBA - Parkour Bassin d'Arcachon

# Google Analytics (optionnel)
NEXT_PUBLIC_GA_ID=G-XXXXXXXXXX

# Google Search Console (optionnel)
NEXT_PUBLIC_GOOGLE_SITE_VERIFICATION=your_verification_code_here 